{
	"name": "taxj",
	"version": "0.0.1",
	"description": "Income tax tools for Vietnamese in Japan",
	"default": "index.html",
	"repository": "git@github.com:taxj/taxj.git",
	"author": "Sang Tran <t@sang.jp>",
	"license": "MIT",
	"source": "src/index.html",
	"browserslist": [
		"> 0.5%",
		"last 2 versions",
		"not dead"
	],
	"scripts": {
		"start": "parcel",
		"build": "parcel build",
		"clean": "rm -rf ./dist ./.parcel-cache",
		"lint": "prettier --check .",
		"lint:fix": "prettier --write ."
	},
	"devDependencies": {
		"@parcel/transformer-sass": "2.8.1",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"parcel": "^2.8.1",
		"prettier": "^2.8.1",
		"process": "^0.11.10",
		"typescript": "^4.9.4"
	},
	"prettier": {
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 2,
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"bracketSpacing": false,
		"arrowParens": "avoid",
		"bracketSameLine": false,
		"jsxSingleQuote": false
	},
	"packageManager": "yarn@3.3.0",
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
